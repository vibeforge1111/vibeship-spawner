{
  "version": 2,
  "mcp_endpoint": "https://mcp.vibeship.co/mcp",
  "description": "Spawner V2 - Cloud-native MCP server for AI-powered development",
  "tools": [
    {
      "name": "spawner_plan",
      "description": "Plan and create new projects (discover + recommend + create)",
      "actions": ["discover", "recommend", "create"]
    },
    {
      "name": "spawner_analyze",
      "description": "Analyze existing codebase for stack/skill recommendations"
    },
    {
      "name": "spawner_context",
      "description": "Load project context and relevant skills"
    },
    {
      "name": "spawner_validate",
      "description": "Run guardrail checks on code"
    },
    {
      "name": "spawner_remember",
      "description": "Save decisions and session progress"
    },
    {
      "name": "spawner_sharp_edge",
      "description": "Query gotchas for current situation"
    },
    {
      "name": "spawner_unstick",
      "description": "Get help when stuck"
    },
    {
      "name": "spawner_templates",
      "description": "List available project templates"
    },
    {
      "name": "spawner_skills",
      "description": "Search and retrieve skills"
    }
  ],
  "templates": [
    {
      "id": "saas",
      "name": "SaaS Starter",
      "description": "Subscription products and dashboards",
      "stack": ["nextjs", "supabase", "stripe", "tailwind"]
    },
    {
      "id": "marketplace",
      "name": "Marketplace",
      "description": "Buy/sell platforms with payments",
      "stack": ["nextjs", "supabase", "stripe", "algolia", "tailwind"]
    },
    {
      "id": "ai-app",
      "name": "AI App",
      "description": "LLM-powered applications",
      "stack": ["nextjs", "supabase", "openai", "tailwind"]
    },
    {
      "id": "web3",
      "name": "Web3 dApp",
      "description": "Blockchain applications",
      "stack": ["nextjs", "wagmi", "viem", "tailwind"]
    },
    {
      "id": "tool",
      "name": "Tool/CLI",
      "description": "CLIs and utilities",
      "stack": ["typescript", "node"]
    }
  ]
}
